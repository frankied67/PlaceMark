<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Monument Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
    {{#each location.monuments}}
    <tr>
      <td>
        {{title}}
      </td>
      <td>
        {{latitude}}
      </td>
      <td>
        {{longitude}}
      </td>
      <td>
        {{category}}
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>